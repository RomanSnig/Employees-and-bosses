{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "require-jsdoc" : 0,
        "max-len": 0,
        "new-cap": 0,
        "camelcase": 0,
        "valid-jsdoc": 0,
        "space-before-blocks": 0
    }
}
